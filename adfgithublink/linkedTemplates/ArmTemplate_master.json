{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfgithublink"
		},
		"des_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'des'"
		},
		"srclink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srclink'"
		},
		"srclinkgit_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srclinkgit'"
		},
		"des_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://webappnonproddata.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfgithublink_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"des_accountKey": {
						"value": "[parameters('des_accountKey')]"
					},
					"srclink_connectionString": {
						"value": "[parameters('srclink_connectionString')]"
					},
					"srclinkgit_connectionString": {
						"value": "[parameters('srclinkgit_connectionString')]"
					},
					"des_properties_typeProperties_url": {
						"value": "[parameters('des_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}